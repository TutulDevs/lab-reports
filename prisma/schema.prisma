generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}


enum Role {
  ADMIN
  STAFF
}


model User {
  id String @id @default(cuid())
  username String @unique
  fullname String
  password String
  role Role @default(STAFF)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}